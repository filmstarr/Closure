{
  "name": "Closure",
  "description": "Require confirmation when closing a tab or navigating away from a site",
  "version": "0.1",
  "permissions": ["contextMenus", "activeTab"],
  "background": {
    "scripts": ["background.js"]
  },
  "manifest_version": 2,
  "web_accessible_resources": ["/img/favicon.ico"],
  "icons": {
    "16": "/img/icon16.png",
    "48": "/img/icon48.png",
    "128": "/img/icon128.png"
  }
}
